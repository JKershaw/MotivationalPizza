<div class="panel panel-default">
		  <div class="panel-heading">
		    <h3 class="panel-title">Done</h3>
		  </div>
		  <div class="panel-body">

		  	<h3>Today</h3>

			<% doneTodayTasks.forEach(function(task, i){  %>
				<div class='task task-done' data-i="<%=i%>" data-task-id='<%=task._id%>'>
					<% 	include task-done-today %>
				</div>
			<% }) %>

			<h3>Older</h3>

			<% doneBeforeTodayTasks.forEach(function(task, i){ %>
				<div class='task task-done archived-task-done' data-task-id='<%=task[i]._id%>'>
					<% 	include task-done-before-today %>
				</div>
			<% }) %>

		  </div>
		</div>